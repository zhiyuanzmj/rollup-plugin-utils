#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zmj/Documents/pluginutils/node_modules/.pnpm/ava@6.2.0_rollup@4.30.1/node_modules/ava/entrypoints/node_modules:/Users/zmj/Documents/pluginutils/node_modules/.pnpm/ava@6.2.0_rollup@4.30.1/node_modules/ava/node_modules:/Users/zmj/Documents/pluginutils/node_modules/.pnpm/ava@6.2.0_rollup@4.30.1/node_modules:/Users/zmj/Documents/pluginutils/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zmj/Documents/pluginutils/node_modules/.pnpm/ava@6.2.0_rollup@4.30.1/node_modules/ava/entrypoints/node_modules:/Users/zmj/Documents/pluginutils/node_modules/.pnpm/ava@6.2.0_rollup@4.30.1/node_modules/ava/node_modules:/Users/zmj/Documents/pluginutils/node_modules/.pnpm/ava@6.2.0_rollup@4.30.1/node_modules:/Users/zmj/Documents/pluginutils/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ava/entrypoints/cli.mjs" "$@"
else
  exec node  "$basedir/../ava/entrypoints/cli.mjs" "$@"
fi
